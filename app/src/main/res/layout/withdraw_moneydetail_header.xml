<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:gravity="center"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:padding="@dimen/x20"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">
            <TextView
                android:layout_centerInParent="true"
                android:drawableRight="@mipmap/arrow_cdr1"
                android:drawablePadding="@dimen/x30"
                android:id="@+id/tv_timeBegin"
                android:text="开始时间"
                android:textColor="#666666"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
        <RelativeLayout
            android:padding="@dimen/x30"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">
            <TextView
                android:layout_centerInParent="true"
                android:drawableRight="@mipmap/arrow_cdr1"
                android:drawablePadding="@dimen/x30"
                android:id="@+id/tv_timeEnd"
                android:text="结束时间"
                android:textColor="#666666"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>
    <View
        android:layout_marginLeft="@dimen/x20"
        android:layout_marginRight="@dimen/x20"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:background="#f2f2f2"
        />
    <RelativeLayout
        android:layout_marginTop="@dimen/x10"
        android:layout_width="match_parent"
        android:layout_marginBottom="@dimen/x10"
        android:layout_height="wrap_content">
        <EditText
            android:id="@+id/fzt_moneyetail_et_serch"
            android:layout_centerVertical="true"
            android:padding="@dimen/x20"
            android:textColor="#999999"
            android:textSize="14sp"
            android:textColorHint="#999999"
            android:hint="输入关键字"
            android:layout_toLeftOf="@+id/fzt_moneydetail_item_img_serch"
            android:background="@null"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x20"
            android:layout_gravity="center"
            android:id="@+id/fzt_moneydetail_item_img_serch"
            android:scaleType="centerCrop"
            android:adjustViewBounds="true"
            android:src="@mipmap/img_search"
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/x50" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x20"
        android:background="#f2f2f2"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/x20"
        >

        <LinearLayout
            android:layout_marginLeft="@dimen/x2"
            android:paddingBottom="@dimen/x40"
            android:paddingTop="@dimen/x40"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="查询合计"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="#999999"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x20"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/fzt_moneyetail_tv_serch_s"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/fontcolor_d8"
                    android:textSize="12sp" />

                <TextView
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" / "
                    android:textColor="#999999"
                    android:textSize="12sp" />
                <TextView
                    android:visibility="gone"
                    android:id="@+id/fzt_moneyetail_tv_serch_z"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9999"
                    android:textColor="@color/fontcolor_d8"
                    android:textSize="12sp" />
            </LinearLayout>
        </LinearLayout>
        <View
            android:layout_width="@dimen/x1"
            android:layout_height="match_parent"
            android:background="#f2f2f2"
            />
        <LinearLayout
            android:layout_marginLeft="@dimen/x2"
            android:paddingBottom="@dimen/x40"
            android:paddingTop="@dimen/x40"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="总提现"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="#999999"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x20"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/fzt_moneyetail_tv_sum_s"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/fontcolor_d8"
                    android:textSize="12sp" />

                <TextView
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" / "
                    android:textColor="#999999"
                    android:textSize="12sp" />
                <TextView
                    android:visibility="gone"
                    android:id="@+id/fzt_moneyetail_tv_sum_z"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9999"
                    android:textColor="@color/fontcolor_d8"
                    android:textSize="12sp" />

            </LinearLayout>


        </LinearLayout>
        <View
            android:layout_width="@dimen/x1"
            android:layout_height="match_parent"
            android:background="#f2f2f2"
            />
        <LinearLayout
            android:layout_marginLeft="@dimen/x2"
            android:paddingBottom="@dimen/x40"
            android:paddingTop="@dimen/x40"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/fzt_moneydetail_item_tvRight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="可提现"
                    android:textColor="#333333"
                    android:textSize="12sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="￥"
                    android:textColor="#999999"
                    android:textSize="12sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/fzt_moneyetail_tv_money"
                android:layout_marginTop="@dimen/x20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/fontcolor_d8"
                android:textSize="12sp" />

        </LinearLayout>
    </LinearLayout>

</LinearLayout>

